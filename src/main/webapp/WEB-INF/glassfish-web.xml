<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE glassfish-web-app PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 Servlet 3.0//EN" "http://glassfish.org/dtds/glassfish-web-app_3_0-1.dtd">
<glassfish-web-app error-url="/page-not-found-404.xhtml">
    <property name="alternatedocroot_1" value="from=/images/* dir=/opt/shared/glassfish/domains/domain1/applications/uploads/posts-files"/>
    <resource-ref>
        <res-ref-name>jdbc/localdbgobpcgob</res-ref-name>
        <jndi-name>jdbc/localdbpcgob</jndi-name>
    </resource-ref>
    <resource-ref>
        <res-ref-name>jdbc/localdbgob</res-ref-name>
        <jndi-name>localdbgob</jndi-name>
    </resource-ref>
    <class-loader delegate="true"/>
    <jsp-config>
        <property name="keepgenerated" value="true">
